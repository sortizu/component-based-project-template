; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[ {
"base": "Node2D",
"class": "Component",
"language": "GDScript",
"path": "res://components/component.gd"
}, {
"base": "Node2D",
"class": "Entity",
"language": "GDScript",
"path": "res://entities/entity.gd"
}, {
"base": "Node2D",
"class": "State",
"language": "GDScript",
"path": "res://components/state_manager/state.gd"
}, {
"base": "Component",
"class": "StateManager",
"language": "GDScript",
"path": "res://components/state_manager/state_manager.gd"
}, {
"base": "Component",
"class": "TestComponent",
"language": "GDScript",
"path": "res://test/test_component.gd"
} ]
_global_script_class_icons={
"Component": "",
"Entity": "",
"State": "",
"StateManager": "",
"TestComponent": ""
}

[application]

config/name="ecs-2d-project-template"
config/icon="res://icon.png"

[autoload]

CBPTUtilities="*res://addons/cbpt/cbpt_utilities.gd"

[editor_plugins]

enabled=PoolStringArray( "res://addons/cbpt/plugin.cfg" )

[gui]

common/drop_mouse_on_gui_input_disabled=true
common/text_edit_undo_stack_max_size=10240

[physics]

common/enable_pause_aware_picking=true

[rendering]

environment/default_environment="res://default_env.tres"
